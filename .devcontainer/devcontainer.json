{
    "name": "flutter_dev",
    "dockerFile": "Dockerfile",
    "forwardPorts": [
        8080
    ],
    "postCreateCommand": "/setup.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.dart-code",
                "dart-code.flutter"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash",
                "terminal.integrated.profiles.linux": {
                    "bash": {
                        "path": "/bin/bash"
                    }
                }
            }
        }
    },
}